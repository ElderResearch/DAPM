    <entry>
      <name>Calc CLA_RISK score</name>
      <description/>
      <type>SQL</type>
      <sql>VACUUM ANALYZE &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score&#x3b;&#xd;&#xa;&#xd;&#xa;--Delete cycl_score entries &#x28;if exist&#x29;&#xd;&#xa;DELETE FROM &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.cycl_score WHERE cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;--Create cycl_score entries&#xd;&#xa;INSERT INTO &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.cycl_score &#x28;cycl_dt, score_cd, parnt_score_cd, score_lvl_num, score_wgt_val, crt_tmstmp&#x29;&#xd;&#xa; SELECT &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;, score_cd, parnt_score_cd, score_lvl_num, curr_score_wgt_val, CURRENT_TIMESTAMP&#xd;&#xa; FROM &#x24;&#x7b;APM_REF_SCHEMA_NAME&#x7d;.score&#x3b;&#xd;&#xa;&#xd;&#xa;--Delete existing CLA_RISK scores &#x28;if exist&#x29;&#xd;&#xa;DELETE FROM &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score WHERE cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27; AND score_cd &#x3d; &#x27;CLA_RISK&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;--Delete scores from claimants that don&#x27;t have certification in previous 13 periods&#xd;&#xa;DELETE FROM &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score&#xd;&#xa;WHERE cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#xd;&#xa;AND enty_type_cd &#x3d; &#x27;CLAIMT&#x27;&#xd;&#xa;AND enty_id IN &#x28;&#xd;&#xa; SELECT enty_id FROM &#x28;&#xd;&#xa;    SELECT enty_score.enty_id, &#x28;&#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27; - max&#x28;cert.cert_perd_end_dt&#x29;&#x29; &#x2f; 7 AS diff&#xd;&#xa;    FROM &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score&#xd;&#xa;   LEFT JOIN &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.claim&#xd;&#xa;    ON enty_score.enty_id &#x3d; claim.claimt_id&#xd;&#xa;   LEFT JOIN &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.cert&#xd;&#xa;     ON claim.claim_id &#x3d; cert.claim_id&#xd;&#xa;   WHERE enty_score.cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#xd;&#xa;   AND enty_score.enty_type_cd &#x3d; &#x27;CLAIMT&#x27;&#xd;&#xa;   GROUP BY enty_score.enty_id&#xd;&#xa; &#x29; AS x&#xd;&#xa; WHERE diff &#x3e; 13 OR diff IS NULL&#xd;&#xa;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;--Calculate normalised score&#xd;&#xa;UPDATE &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score es1&#xd;&#xa;SET normlzd_score_val &#x3d; &#x28;&#xd;&#xa;  WITH cnt AS &#x28;&#xd;&#xa;    SELECT COUNT&#x28;DISTINCT enty_id&#x29; AS claimt_cnt&#xd;&#xa;    FROM nrd.enty_score&#xd;&#xa;   WHERE enty_type_cd &#x3d; &#x27;CLAIMT&#x27;&#xd;&#xa;    AND cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#xd;&#xa;  &#x29;,&#xd;&#xa; agg AS &#x28;&#xd;&#xa;   SELECT es.score_cd,&#xd;&#xa;   CASE WHEN COUNT&#x28;&#x2a;&#x29; &#x3d; cnt.claimt_cnt&#xd;&#xa;    THEN MIN&#x28;es.score_val&#x29;&#xd;&#xa;    ELSE 0&#xd;&#xa;   END AS min,&#xd;&#xa;   CASE WHEN COUNT&#x28;&#x2a;&#x29; &#x3d; cnt.claimt_cnt&#xd;&#xa;    THEN MAX&#x28;es.score_val&#x29; - MIN&#x28;es.score_val&#x29;&#xd;&#xa;    ELSE MAX&#x28;es.score_val&#x29;&#xd;&#xa;   END AS range&#xd;&#xa;    FROM &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score es&#xd;&#xa;    CROSS JOIN cnt&#xd;&#xa;    WHERE es.enty_type_cd &#x3d; &#x27;CLAIMT&#x27;&#xd;&#xa;   AND es.cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#xd;&#xa;   GROUP BY es.score_cd, cnt.claimt_cnt&#xd;&#xa;  &#x29;&#xd;&#xa;  SELECT&#xd;&#xa;  CASE WHEN agg.range &#x3e; 0&#xd;&#xa;   THEN &#x28;&#x28;score_val - agg.min&#x29; &#x2f; agg.range&#x29; &#x2a; 100&#xd;&#xa;  ELSE 100&#xd;&#xa; END&#xd;&#xa; FROM &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score es2&#xd;&#xa; JOIN agg&#xd;&#xa;   ON es2.score_cd &#x3d; agg.score_cd&#xd;&#xa;  WHERE es1.cycl_dt &#x3d; es2.cycl_dt&#xd;&#xa;    AND es1.enty_type_cd &#x3d; es2.enty_type_cd&#xd;&#xa;    AND es1.enty_id &#x3d; es2.enty_id&#xd;&#xa;    AND es1.score_cd &#x3d; es2.score_cd&#xd;&#xa;&#x29;&#xd;&#xa;WHERE es1.enty_type_cd &#x3d; &#x27;CLAIMT&#x27;&#xd;&#xa;AND es1.cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;--Calculate weighted score&#xd;&#xa;UPDATE &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score es1 &#xd;&#xa;SET &#x28;wgtd_score_val, last_updt_tmstmp&#x29; &#x3d; &#x28;&#xd;&#xa;  &#x28;&#xd;&#xa;    SELECT es1.normlzd_score_val &#x2a; s1.curr_score_wgt_val &#x2f; SUM&#x28;s2.curr_score_wgt_val&#x29;&#xd;&#xa;   FROM &#x24;&#x7b;APM_REF_SCHEMA_NAME&#x7d;.score s2&#xd;&#xa;   WHERE s2.parnt_score_cd &#x3d; s1.parnt_score_cd&#xd;&#xa;    AND s2.enty_type_cd &#x3d; &#x27;CLAIMT&#x27;       &#xd;&#xa;  &#x29;,&#xd;&#xa; CURRENT_TIMESTAMP&#xd;&#xa;&#x29;&#xd;&#xa;FROM &#x24;&#x7b;APM_REF_SCHEMA_NAME&#x7d;.score s1&#xd;&#xa;WHERE es1.cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#xd;&#xa;AND s1.enty_type_cd &#x3d; &#x27;CLAIMT&#x27;&#xd;&#xa;AND s1.parnt_score_cd &#x3d; &#x27;CLA_RISK&#x27;&#xd;&#xa;AND es1.score_cd &#x3d; s1.score_cd&#x3b;&#xd;&#xa;&#xd;&#xa;--Sum to CLA_RISK score&#xd;&#xa;INSERT INTO &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score &#x28;cycl_dt, enty_type_cd, enty_id, score_cd, score_val, crt_tmstmp&#x29;&#xd;&#xa;SELECT &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;, &#x27;CLAIMT&#x27;, es.enty_id, &#x27;CLA_RISK&#x27;, SUM&#x28;es.wgtd_score_val&#x29;, CURRENT_TIMESTAMP&#xd;&#xa;FROM &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score es&#xd;&#xa;JOIN &#x24;&#x7b;APM_REF_SCHEMA_NAME&#x7d;.score s&#xd;&#xa; ON es.score_cd &#x3d; s.score_cd&#xd;&#xa; AND es.enty_type_cd &#x3d; s.enty_type_cd&#xd;&#xa; AND s.parnt_score_cd &#x3d; &#x27;CLA_RISK&#x27;&#xd;&#xa;WHERE es.cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#xd;&#xa;AND es.enty_type_cd &#x3d; &#x27;CLAIMT&#x27;&#xd;&#xa;GROUP BY es.enty_id&#x3b;&#xd;&#xa;&#xd;&#xa;--Calculate normalised CLA_RISK score&#xd;&#xa;UPDATE &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score es1 &#xd;&#xa;SET normlzd_score_val &#x3d; &#x28;&#xd;&#xa;  WITH agg AS &#x28;&#xd;&#xa;    SELECT&#xd;&#xa;    MIN&#x28;score_val&#x29; AS min,&#xd;&#xa;    MAX&#x28;score_val&#x29; - MIN&#x28;score_val&#x29; AS range&#xd;&#xa;    FROM &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score&#xd;&#xa;   WHERE enty_type_cd &#x3d; &#x27;CLAIMT&#x27;&#xd;&#xa;    AND cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#xd;&#xa;    AND score_cd &#x3d; &#x27;CLA_RISK&#x27;&#xd;&#xa;  &#x29;&#xd;&#xa;  SELECT&#xd;&#xa;  CASE WHEN agg.range &#x3e; 0&#xd;&#xa;   THEN &#x28;&#x28;score_val - agg.min&#x29; &#x2f; agg.range&#x29; &#x2a; 100&#xd;&#xa;  ELSE 100&#xd;&#xa; END&#xd;&#xa; FROM &#x24;&#x7b;APM_NRD_SCHEMA_NAME&#x7d;.enty_score es2&#xd;&#xa; CROSS JOIN agg&#xd;&#xa;  WHERE es1.cycl_dt &#x3d; es2.cycl_dt&#xd;&#xa;    AND es1.enty_type_cd &#x3d; es2.enty_type_cd&#xd;&#xa;    AND es1.enty_id &#x3d; es2.enty_id&#xd;&#xa;    AND es1.score_cd &#x3d; es2.score_cd&#xd;&#xa;&#x29;&#xd;&#xa;WHERE es1.enty_type_cd &#x3d; &#x27;CLAIMT&#x27;&#xd;&#xa;AND es1.cycl_dt &#x3d; &#x27;&#x24;&#x7b;APM_CYCL_DT&#x7d;&#x27;&#xd;&#xa;AND es1.score_cd &#x3d; &#x27;CLA_RISK&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
