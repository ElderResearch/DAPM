        <class_name>IsValidDate</class_name>

        <class_source><![CDATA[import java.text.SimpleDateFormat;
import java.util.Date;
import java.util.regex.Matcher;
import java.util.regex.Pattern;
import java.net.InetAddress;

private final static SimpleDateFormat DATE_FORMAT = new SimpleDateFormat("yyyy-MM-dd'T'HH:mm:ss");
private final static String IPV4_REGEX = 
		"^([01]?\\d\\d?|2[0-4]\\d|25[0-5])\\." +
		"([01]?\\d\\d?|2[0-4]\\d|25[0-5])\\." +
		"([01]?\\d\\d?|2[0-4]\\d|25[0-5])\\." +
		"([01]?\\d\\d?|2[0-4]\\d|25[0-5])$";
private final static Pattern IPV4_PATTERN = Pattern.compile(IPV4_REGEX);

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {

	DATE_FORMAT.setLenient(false);

	if (first) {
      first=false;
    }

    Object[] r = getRow();
    if (r == null) {
        setOutputDone();
        return false;
    }
    r = createOutputRow(r, data.outputRowMeta.size());
     
    // Get the value from an input field
	String ipv4 = get(Fields.In, "ipv4_net_val").getString(r);
    String param1In = get(Fields.In, getParameter("param_1_in")).getString(r);
	String param2In = get(Fields.In, getParameter("param_2_in")).getString(r);
	String param3In = get(Fields.In, getParameter("param_3_in")).getString(r);
	try {
		//check ipv4 exists
		if (ipv4 == null) {
			throw new Exception();
		}

		//parse input 1
		if (param1In != null && param1In != "") {
			if (param1In.contains("/")) {
				param1In = param1In.split("/")[0];
			}
			Matcher matcher = IPV4_PATTERN.matcher(param1In);
	  		if (!matcher.matches()) {
				throw new Exception();
			}
			if (param1In.equals("127.0.0.1") || param1In.startsWith("10.") || param1In.startsWith("192.168.")
					|| param1In.startsWith("172.16.") || param1In.startsWith("172.17.") || param1In.startsWith("172.18.")
					|| param1In.startsWith("172.19.") || param1In.startsWith("172.20.") || param1In.startsWith("172.21.")
					|| param1In.startsWith("172.22.") || param1In.startsWith("172.23.") || param1In.startsWith("172.24.")
					|| param1In.startsWith("172.25.") || param1In.startsWith("172.26.") || param1In.startsWith("172.27.")
					|| param1In.startsWith("172.28.") || param1In.startsWith("172.29.") || param1In.startsWith("172.30.")
					|| param1In.startsWith("172.31.")) {
				throw new Exception();
			}
		} else {
			throw new Exception();
		}

   		//parse input 2
		if (param2In != null && param2In != "") {
			get(Fields.Out, getParameter("param_2_out")).setValue(r, Double.parseDouble(param2In));
		}

   		//parse input 3
		if (param3In != null && param3In != "") {
			get(Fields.Out, getParameter("param_3_out")).setValue(r, Double.parseDouble(param3In));
		}
	
	} catch (Exception e) {
		//set field to null and set err_flag to 1
		get(Fields.Out, getParameter("param_2_out")).setValue(r, null);
		get(Fields.Out, getParameter("param_3_out")).setValue(r, null);
		get(Fields.Out, "err_flag").setValue(r, 1L);
	}

    // Send the row on to the next step.
    putRow(data.outputRowMeta, r);
    return true;
}



]]></class_source>
