      <description/>
      <type>SQL</type>
      <sql>DROP TABLE IF EXISTS &#x24;&#x7b;APM_STG_SCHEMA_NAME&#x7d;.&#x24;&#x7b;TBL_STG_KBA_USER_REPOSITORY&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE TABLE &#x24;&#x7b;APM_STG_SCHEMA_NAME&#x7d;.&#x24;&#x7b;TBL_STG_KBA_USER_REPOSITORY&#x7d; &#x28;&#xd;&#xa;	id serial primary key,&#xd;&#xa;	ssn character varying,&#xd;&#xa;	first_name character varying,&#xd;&#xa;	middle_init character varying,&#xd;&#xa;	last_name character varying,&#xd;&#xa;	address_line1 character varying,&#xd;&#xa;	address_line2 character varying,&#xd;&#xa;	city character varying,&#xd;&#xa;	county_id character varying,&#xd;&#xa;	state_id character varying,&#xd;&#xa;	postal_code character varying,&#xd;&#xa;	resident_city_id character varying,&#xd;&#xa;	resident_state_id character varying,&#xd;&#xa;	resident_postal_code character varying,&#xd;&#xa;	home_phone character varying,&#xd;&#xa;	home_phone_ext character varying,&#xd;&#xa;	email character varying,&#xd;&#xa;	password character varying,&#xd;&#xa;	pin_nbr character varying,&#xd;&#xa;	mothers_maiden_name character varying,&#xd;&#xa;	login_attempt_nbr character varying,&#xd;&#xa;	login_attempt_nbr_int integer,&#xd;&#xa;	last_login_dt character varying,&#xd;&#xa;	last_login_dt_ts timestamp without time zone,&#xd;&#xa;	entry_dt character varying,&#xd;&#xa;	entry_dt_ts timestamp without time zone,&#xd;&#xa;	profile_last_updt_dt character varying,&#xd;&#xa;	profile_last_updt_dt_ts timestamp without time zone,&#xd;&#xa;	err_flag integer,&#xd;&#xa;	crt_tmstmp timestamp default current_timestamp not null&#xd;&#xa;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;ALTER TABLE &#x24;&#x7b;APM_STG_SCHEMA_NAME&#x7d;.&#x24;&#x7b;TBL_STG_KBA_USER_REPOSITORY&#x7d; OWNER TO &#x24;&#x7b;APM_ROLE&#x7d;&#x3b;</sql>
