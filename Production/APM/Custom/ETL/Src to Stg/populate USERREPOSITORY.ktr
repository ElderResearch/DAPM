        <class_name>IsValidDate</class_name>

        <class_source><![CDATA[import java.text.SimpleDateFormat;
import java.util.Date;

private final static SimpleDateFormat DATE_FORMAT = new SimpleDateFormat("yyyy-MM-dd'T'HH:mm:ss");

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {

	DATE_FORMAT.setLenient(false);

	if (first) {
      first=false;
    }

    Object[] r = getRow();
    if (r == null) {
        setOutputDone();
        return false;
    }
    r = createOutputRow(r, data.outputRowMeta.size());
     
    // Get the value from an input field
	String ssn = get(Fields.In, "ssn").getString(r);
    String param1In = get(Fields.In, getParameter("param_1_in")).getString(r);
    String param2In = get(Fields.In, getParameter("param_2_in")).getString(r);
	String param3In = get(Fields.In, getParameter("param_3_in")).getString(r);
	String param4In = get(Fields.In, getParameter("param_4_in")).getString(r);
	String param5In = get(Fields.In, getParameter("param_5_in")).getString(r);
	String param6In = get(Fields.In, getParameter("param_6_in")).getString(r);
	try {
		//check SSN exists and is 9 digits
		if (ssn == null || ssn.length() != 9) {
			throw new Exception();
		}
		long ssnL = Long.parseLong(ssn);
		if (ssnL <= 0) {
			throw new Exception();
		}
   		//parse input 1
		if (param1In != null && param1In != "") {
			get(Fields.Out, getParameter("param_1_out")).setValue(r, DATE_FORMAT.parse(param1In));
		}
   		//parse input 2
		if (param2In != null && param2In != "") {
			get(Fields.Out, getParameter("param_2_out")).setValue(r, DATE_FORMAT.parse(param2In));
		}
   		//parse input 3
		if (param3In != null && param3In != "") {
			get(Fields.Out, getParameter("param_3_out")).setValue(r, DATE_FORMAT.parse(param3In));
		}
   		//parse input 4
		if (param4In != null && param4In != "") {
			get(Fields.Out, getParameter("param_4_out")).setValue(r, Long.parseLong(param4In));
		}
   		//parse input 5
		if (param5In != null && param5In != "") {
			String phone = param5In.replaceAll("/", "").replaceAll("-", "");
			if (!"9999999999".equals(phone) && !"0000000000".equals(phone)) {
				get(Fields.Out, getParameter("param_5_in")).setValue(r, phone);
				if (phone.length() != 10) {
					throw new Exception();
				}
				if (phone.startsWith("0") || phone.startsWith("1")) {
					throw new Exception();
				}
				Long.parseLong(phone);
			} else {
				get(Fields.Out, getParameter("param_5_in")).setValue(r, null);
			}
		}
   		//parse input 6
		if (param6In != null && param6In != "") {
			String email = null;
			if (!"none".equals(param6In) && !"n/a".equals(param6In)) {
				email = param6In;
				if (!email.contains("@")) {
					throw new Exception();
				}
			}
			get(Fields.Out, getParameter("param_6_in")).setValue(r, email);
		}
	} catch (Exception e) {
		//set field to null and set err_flag to 1
		get(Fields.Out, getParameter("param_1_out")).setValue(r, null);
		get(Fields.Out, getParameter("param_2_out")).setValue(r, null);
		get(Fields.Out, getParameter("param_3_out")).setValue(r, null);
		get(Fields.Out, getParameter("param_4_out")).setValue(r, null);
		get(Fields.Out, "err_flag").setValue(r, 1L);
	}

    // Send the row on to the next step.
    putRow(data.outputRowMeta, r);
    return true;
}




]]></class_source>
